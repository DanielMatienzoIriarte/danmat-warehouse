{
  "name": "sales",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon ./src/server.ts",
    "build": "rm -rf build/ && tsc -p .",
    "babel-build": "babel src --out-dir dist",
    "start": "NODE_ENV=production nodemon build/server.js",
    "test": "jest --coverage",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm-generate-migration": "npm run typeorm migration:generate -- -d ./src/config/data-source.ts ./src/database/migrations/$npm_config_name",
    "typeorm-run-migrations": "npm run typeorm migration:run -- -d ./src/config/data-source.ts",
    "typeorm-revert-migrations": "tsc && typeorm  -d dist/config/data-source.js migration:revert",
    "typeorm-run-seeders": "npx ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d src/config/data-source.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/validator": "^13.15.3",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.1",
    "morgan": "^1.10.1",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "typeorm": "^0.3.27",
    "typeorm-extension": "^3.7.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@types/connect": "^3.4.38",
    "@types/cors": "^2.8.19",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.9.1",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2"
  },
  "lint-staged": {
    "*.ts": "yarn lint"
  }
}
